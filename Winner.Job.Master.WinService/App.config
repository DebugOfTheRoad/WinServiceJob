<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
  </startup>
  <appSettings>

    <!--数据库连接字符串是否加密-->
    <add key="ConnectionStringEncrypt" value="false" />

    <!--是否为开发模式-->
    <add key="Debuger" value="true"/>

    <!--服务队列-->
    <add key="Winner.Job.Master.Queue" value="2"/>
  </appSettings>
</configuration>
